<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation.xml"
    app:startDestination="@id/splashScreenFragment">

    <fragment
        android:id="@+id/signUpFragment"
        android:name="com.the_mystic.diu.blooddonationproject2.fragment.auth.SignUpFragment"
        android:label="fragment_sign_up"
        tools:layout="@layout/fragment_sign_up" >
        <action
            android:id="@+id/action_signUpFragment_to_accountSetUpFragment"
            app:destination="@id/accountSetUpFragment" />
        <action
            android:id="@+id/action_signUpFragment_to_signInFragment"
            app:destination="@id/signInFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.the_mystic.diu.blooddonationproject2.fragment.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_blood_requestFragment"
            app:destination="@id/blood_requestFragment" />
        <action
            android:id="@+id/action_homeFragment_to_donorLiveMapFragment"
            app:destination="@id/donorLiveMapFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.the_mystic.diu.blooddonationproject2.fragment.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/signInFragment"
        android:name="com.the_mystic.diu.blooddonationproject2.fragment.auth.SignInFragment"
        android:label="fragment_sign_in"
        tools:layout="@layout/fragment_sign_in" >
        <action
            android:id="@+id/action_signInFragment_to_signUpFragment"
            app:destination="@id/signUpFragment" />
        <action
            android:id="@+id/action_signInFragment_to_newHomeFragment"
            app:destination="@id/newHomeFragment" />
    </fragment>
    <fragment
        android:id="@+id/blood_requestFragment"
        android:name="com.the_mystic.diu.blooddonationproject2.fragment.blood_requestFragment"
        android:label="fragment_blood_request"
        tools:layout="@layout/fragment_blood_request" />
    <fragment
        android:id="@+id/accountSetUpFragment"
        android:name="com.the_mystic.diu.blooddonationproject2.fragment.AccountSetUpFragment"
        android:label="fragment_account_set_up"
        tools:layout="@layout/fragment_account_set_up" >
        <action
            android:id="@+id/action_accountSetUpFragment_to_newHomeFragment"
            app:destination="@id/newHomeFragment" />
    </fragment>
    <fragment
        android:id="@+id/splashScreenFragment"
        android:name="com.the_mystic.diu.blooddonationproject2.fragment.SplashScreenFragment"
        android:label="fragment_splash_screen"
        tools:layout="@layout/fragment_splash_screen" >
        <action
            android:id="@+id/action_splashScreenFragment_to_signInFragment"
            app:destination="@id/signInFragment" />
        <action
            android:id="@+id/action_splashScreenFragment_to_newHomeFragment"
            app:destination="@id/newHomeFragment" />
    </fragment>
    <fragment
        android:id="@+id/donorLiveMapFragment"
        android:name="com.the_mystic.diu.blooddonationproject2.fragment.DonorLiveMapFragment"
        android:label="fragment_donor_live_map"
        tools:layout="@layout/fragment_donor_live_map" />
    <fragment
        android:id="@+id/newHomeFragment"
        android:name="com.the_mystic.diu.blooddonationproject2.fragment.NewHomeFragment"
        android:label="fragment_new_home"
        tools:layout="@layout/fragment_new_home" >
        <action
            android:id="@+id/action_newHomeFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_newHomeFragment_to_blood_requestFragment"
            app:destination="@id/blood_requestFragment" />
        <action
            android:id="@+id/action_newHomeFragment_to_bloodRequestListFragment"
            app:destination="@id/bloodRequestListFragment" />
        <action
            android:id="@+id/action_newHomeFragment_to_userListFragment"
            app:destination="@id/userListFragment" />
        <action
            android:id="@+id/action_newHomeFragment_to_hospitalFragment"
            app:destination="@id/hospitalFragment" />
        <action
            android:id="@+id/action_newHomeFragment_to_orgFragment"
            app:destination="@id/orgFragment" />
        <action
            android:id="@+id/action_newHomeFragment_to_dieaseFragment"
            app:destination="@id/dieaseFragment" />
        <action
            android:id="@+id/action_newHomeFragment_to_profileFragment"
            app:destination="@id/profileFragment" />

    </fragment>
    <fragment
        android:id="@+id/bloodRequestListFragment"
        android:name="com.the_mystic.diu.blooddonationproject2.fragment.BloodRequestListFragment"
        android:label="fragment_blood_request_list"
        tools:layout="@layout/fragment_blood_request_list" />
    <fragment
        android:id="@+id/userListFragment"
        android:name="com.the_mystic.diu.blooddonationproject2.fragment.UserListFragment"
        android:label="fragment_user_list"
        tools:layout="@layout/fragment_user_list" />
    <fragment
        android:id="@+id/hospitalFragment"
        android:name="com.the_mystic.diu.blooddonationproject2.fragment.HospitalFragment"
        android:label="fragment_hospital"
        tools:layout="@layout/fragment_hospital" />
    <fragment
        android:id="@+id/orgFragment"
        tools:layout="@layout/fragment_org"
        android:name="com.the_mystic.diu.blooddonationproject2.fragment.OrgFragment"
        android:label="OrgFragment" />
    <fragment
        android:id="@+id/dieaseFragment"
        tools:layout="@layout/fragment_diease"
        android:name="com.the_mystic.diu.blooddonationproject2.fragment.DieaseFragment"
        android:label="DieaseFragment" />
</navigation>